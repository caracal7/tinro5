{
    "name": "tinro5",
    "version": "0.0.6",
    "description": "tinro5 is a tiny declarative router for Svelte 5",
    "main": "dist/tinro.js",
    "module": "dist/tinro.es.js",
    "svelte": "src/index.js",
    "types": "index.d.ts",
    "files": [
        "dist/**",
        "src/**",
        "index.d.ts",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "svelte": "./src/index.js",
            "import": "./dist/tinro.es.js",
            "require": "./dist/tinro.js",
            "default": "./dist/tinro.es.js"
        }
    },
    "scripts": {
        "build": "node esbuild",
        "prepublishOnly": "npm run build",
        "dev": "node tests/esbuild.dev --dev",
        "compare": "npm run test:build && node tests/esbuild.compare",
        "pretest": "node tests/esbuild.dev",
        "test": "node tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/caracal7/tinro5.git"
    },
    "keywords": [
        "tinro",
        "tinro5",
        "svelte-router",
        "router",
        "svelte"
    ],
    "author": "Alexey Schebelev",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/caracal7/tinro5/issues"
    },
    "homepage": "https://github.com/caracal7/tinro5#readme",
    "devDependencies": {
        "derver": "^0.5.3",
        "esbuild": "^0.19.0",
        "esbuild-svelte": "^0.9.1",
        "port-authority": "^1.1.2",
        "puppeteer": "^21.0.0",
        "svelte": "^5.23.0",
        "uvu": "^0.5.3"
    }
}
